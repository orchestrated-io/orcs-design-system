{
  "teamId": "ck0ronzww000002s6cht86oyw",
  "projectId": "sAsdXnTMxO",
  "framework": "react",
  "examplePaths": [
    "lib/components/*/*.stories.js",
    "lib/components/Typography/Typography.stories.mdx"
  ],
  "includePaths": [],
  "deployPaths": [],
  "packages": [
    {
      "ignore": ["systemtheme", "styleLink", "getOptionByValue"],
      "name": "orcs-design-system",
      "relativePath": "package.json",
      "src": {
        "entry": "lib/interplayIndex.js"
      }
    }
  ],
  "exampleMatchers": [
    {
      "include": "**/*.stories.js",
      "matchPatterns": ["{componentName}/", ".*/{componentName}."],
      "matchProperty": "relativePath"
    },
    {
      "include": "lib/components/Typography/Typography.stories.mdx",
      "matchPatterns": ["H1|H2|H3|H4|H5|H6|P|Text|Code|Quote"],
      "matchProperty": "name"
    }
  ],
  "nestedExampleLimit": 1,
  "peerDependencies": {
    "styled-components": "^5.2.1"
  }
}
