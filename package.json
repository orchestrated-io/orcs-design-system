    "react-dates": "^21.8.0",
    "react-intersection-observer": "^9.4.3",
    "react-moment-proptypes": "^1.8.1",
    "react-number-format": "^5.3.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.7.4",
    "styled-system": "^5.1.5"
