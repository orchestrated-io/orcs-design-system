{
  "name": "orca-v2",
  "version": "0.0.1-alpha.0",
  "description": "Orchestrated Frontend React NPM library for Brand designs",
  "main": "dist/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "audit-ci": "audit-ci -m",
    "lint": "eslint lib/",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "build": "styleguidist build",
    "dist": "babel lib -d dist && npm run module",
    "module": "BABEL_ENV=es babel lib -d es",
    "deploy-stack": "serverless deploy",
    "deploy-client": "serverless client deploy  --no-confirm",
    "storybook": "start-storybook -s ./lib/assets",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic",
    "chromatic:ci": "chromatic --exit-zero-on-changes",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@storybook/preset-create-react-app": "^1.5.2",
    "polished": "^3.4.4",
    "prop-types": "^15.6.2",
    "react-dates": "^20.3.0",
    "react-number-format": "^4.3.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "styled-components-grid": "^2.2.2",
    "styled-is": "^1.3.0",
    "styled-system": "^5.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@storybook/addon-a11y": "^5.3.12",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-notes": "^5.3.12",
    "@storybook/addon-storysource": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^6.0.0-alpha.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "audit-ci": "^2.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "moment": "^2.24.0",
    "moment-jalaali": "^0.7.4",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "serverless": "^1.62.0",
    "serverless-finch": "^2.5.2",
    "storybook-chromatic": "^3.5.0",
    "style-loader": "^1.1.3",
    "styled-components": "^4.4.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "react": ">=0.16",
    "react-dom": ">=0.16",
    "styled-components": "^4.4.0"
  },
  "files": [
    "dist",
    "es"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-push": "./installGitSecrets.sh &&  git secrets --register-aws && git-secrets --scan -r && npm run audit-ci",
      "pre-commit": "prettier --write lib/**/*.js* && npm run lint"
    }
  }
}
